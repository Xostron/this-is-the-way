{
  "name": "ultramar-api",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./src/bin/www",
    "dev": "nodemon ./src/bin/www"
  },
  "nodemonConfig": {
    "ignore": [
      "public/*",
      "dump/*"
    ]
  },
  "_moduleAliases": {
    "@root": "src/",
    "@tool": "src/tool",
    "@api": "src/routes/api",
    "@dict": "src/dict",
    "@exceptions": "src/exceptions",
    "@middlew": "src/middlew",
    "@store": "src/store",
    "@util": "util/"
  },
  "dependencies": {
    "axios": "^1.7.7",
    "bcrypt": "^5.1.1",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^16.4.5",
    "express": "~4.16.1",
    "express-fileupload": "^1.5.1",
    "http-errors": "~1.6.3",
    "jsonwebtoken": "^9.0.2",
    "module-alias": "^2.2.3",
    "mongojs": "^3.1.0",
    "morgan": "~1.9.1",
    "pug": "^3.0.3",
    "uuid4": "^2.0.3"
  },
  "devDependencies": {
    "nodemon": "^3.1.7"
  }
}
