{
	"compilerOptions": {
		"moduleResolution": "node",
		"baseUrl": ".",
		"allowJs": true,
		"module": "commonjs",
		"paths": {
			"@root/*": ["src/*"],
			"@routes": ["src/routes/*"],
			"@socket": ["src/socket/*"],
			"@middlew": ["src/middlew/*"],
			"@dict/*": ["src/dict/*"],
			"@tool/*": ["src/tool/*"],
			"@util": ["src/util/*"],
			"@store/*": ["src/store/*"]
		}
	}
}
