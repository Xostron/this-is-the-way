{
	"compilerOptions": {
		"moduleResolution": "node",
		"baseUrl": ".",
		"allowJs": true,
		"module": "commonjs",
		"paths": {
			"@root/*": ["src/*"],
			"@routes/*": ["src/routes/*"],
			"@socket/*": ["src/socket/*"],
			"@service/*": ["src/service/*"],
			"@middlew/*": ["src/middlew/*"],
			"@dict/*": ["src/dict/*"],
			"@tool/*": ["src/tool/*"],
			"@util/*": ["src/util/*"],
			"@store/*": ["src/store/*"]
		}
	}
}
